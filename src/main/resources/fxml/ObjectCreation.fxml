<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.*?>
<GridPane xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="com.silverhetch.calisto.javafx.object.ObjectCreation">
    <ListView fx:id="fileList"/>
    <fx:include source="TagSelection.fxml" fx:id="tagSelection" GridPane.rowIndex="0" GridPane.columnIndex="1"/>
    <JFXButton onMouseClicked="#onConfirm" text="%app.create" GridPane.columnIndex="1" GridPane.rowIndex="1"
               alignment="CENTER_RIGHT"/>
</GridPane>
