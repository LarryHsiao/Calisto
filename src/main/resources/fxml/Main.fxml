<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.BorderPane?>
<BorderPane prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8.0.121"
            fx:id="rootPane"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.silverhetch.calisto.javafx.Main">
    <top>
        <MenuBar BorderPane.alignment="CENTER">
            <Menu text="File">
                <MenuItem text="%menuBar.objectList" onAction="#objectList"/>
                <MenuItem text="%menuBar.tagManagement" onAction="#tagManagement"/>
                <MenuItem text="%menuBar.options" onAction="#options"/>
            </Menu>
        </MenuBar>
    </top>

</BorderPane>
